{
  "dpi": 300,
  "max_retries": 3,
  "retry_delay": 5,
  "vlm_model": "qwen/qwen3-vl-8b",
  "vlm_endpoint": "http://localhost:1234/v1",
  "enable_multi_record": true,
  "enable_duplicate_detection": true,
  "enable_validation": true,
  "export_excel": true,
  "export_markdown": true,
  "export_json": true,
  "batch_size": 10,
  "parallel_workers": 1,
  "log_level": "INFO",
  "enable_chain_of_thought": true,
  "enable_visual_grounding": true,
  "enable_adaptive_temperature": true,
  "enable_field_metadata_tracking": true,
  "enable_validation_stage": false,
  "enable_self_correction": false,
  "enable_consensus_for_critical_fields": false,
  "critical_field_keywords": [
    "id",
    "number",
    "code",
    "mrn",
    "ssn",
    "date",
    "dob",
    "amount",
    "charge",
    "total",
    "balance"
  ],
  "validation_confidence_threshold": 0.85,
  "retry_temperature_increment": 0.05,
  "max_temperature": 0.3
}
